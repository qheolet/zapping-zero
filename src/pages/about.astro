---
import Layout from "../layouts/BlogPost.astro";

// Import a single file
import * as about from "../content/page/About.mdx";
import type { MarkdownHeading } from "astro";

const Content = about.Content;
const heading: MarkdownHeading[] = about.getHeadings();
---

<Layout
  title={about.frontmatter.title}
  description=""
  heroImage={about.frontmatter.heroImage}
  readTime=""
  headings={heading}
>
  <Content />
</Layout>
